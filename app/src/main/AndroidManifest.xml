<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.owner.musicplay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".nardActivity"
            android:label="Lili Nard"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".tankActivity"
            android:label="T.A.N.K"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".dumoActivity"
            android:label="Dumo Squad"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".inflatorsActivity"
            android:label="The Inflators"
            android:parentActivityName=".MainActivity"></activity>
        <meta-data android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity"/>
    </application>

</manifest>